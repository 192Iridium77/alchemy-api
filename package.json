{
  "name": "alchemy-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/server.ts | bunyan --color -o short",
    "create-db": "ts-node ./src/services/db/create | bunyan --color",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.migrations.js",
    "migrate:down": "knex migrate:down --knexfile knexfile.migrations.js",
    "migrate:make": "knex migrate:make --knexfile knexfile.migrations.js",
    "prod:create-db": "NODE_ENV=production ts-node ./src/services/db/create | bunyan --color",
    "prod:migrate:latest": "knex migrate:latest --knexfile knexfile.migrations.js --env production",
    "build": "rm -rf dist && tsc",
    "check-circular-deps": "madge --circular --extensions ts ./src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "bcrypt": "^5.1.0",
    "bunyan": "^1.8.15",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "openai": "^3.2.1",
    "pg": "^8.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.10",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "madge": "^6.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0"
  }
}
